<% if @publisher_logo_image_primary %>
	$('#render_publisher_logo_image_primary').html("<%= j render('publishers/publisher_logo_image_primary') %>");
	$(".container-figure-logo").removeClass("container-figure-image-border").addClass("container-figure-image-border-edit");
	// $('#render_publisher_logo_image_primary .container-menu').html("<#%= j render('publishers/publisher_logo_image_primary_menu') %#>");
	$('#render_publisher_logo_image_primary_crop').html("<%= j render('publishers/publisher_logo_image_primary_crop') %>");
	id_logo_image = <%= @id_logo_image %>;
	$(".drop-2-container").removeClass("hidden");	
	$(".container-figure-logo figure").removeClass("cursor-pointer");
	closeAddLogoImagePrimary();
<% else %>
	alert("Upload error");
<% end %>

