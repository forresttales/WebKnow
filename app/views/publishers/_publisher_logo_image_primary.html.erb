<script>

	var drop_visible_namespace201 = false;
	function drop_show_namespace201() {
		$(".drop-namespace201 .drop-201-container").removeClass('drop-hidden');
		$(".drop-namespace201 .drop-201-box").fadeIn(500).removeClass("margin-top-hide-1").addClass("margin-top-show-1");
		drop_visible_namespace201 = true;			
	}	
	function drop_hide_namespace201() {
		$(".drop-namespace201 .drop-201-box").removeClass("margin-top-show-1").addClass("margin-top-hide-1").fadeOut(50);		
		drop_visible_namespace201 = false;			
	}	
	function drop_toggle_namespace201() {
		if (drop_visible_namespace201) {
			drop_hide_namespace201();
		}
		else {
			drop_close_all_called_namespace201 = false;
			drop_show_namespace201();
		}
	}	
	
	function dropCloseAll_namespace201() {
		drop_close_all_called_namespace201 = true;
		drop_hide_namespace201();
	}
	
	
	var drop_close_all_called_namespace201 = false;
	
	
	$(document).ready(function () {				

		$(document).on('click', function(e) {
			// var target_namespace201 = $(e.target).parents('#drop_namespace201').attr("id");
			var target_namespace201 = $(e.target).parents('.drop-namespace201').attr("class");
			if (target_namespace201 == null) {
				drop_hide_namespace201();
			}
		});

		$(".drop-namespace201 .drop-201-target").on('click', function() {
			drop_toggle_namespace201();
		});
		$(".drop-namespace201 .drop-201-link-1[data-id='1']").on("click", function() {
			dropCloseAll_namespace201();
			$(document).trigger("btn_logo_image_primary_change_click");
		});
		$(".drop-namespace201 .drop-201-link-1[data-id='2']").on("click", function() {
			dropCloseAll_namespace201();
			$(document).trigger("btn_logo_image_primary_crop_click");
		});
		$(".drop-namespace201 .drop-201-link-1[data-id='3']").on("click", function() {
			dropCloseAll_namespace201();
			$(document).trigger("btn_logo_image_primary_delete_click");
		});

	});
	
</script>


