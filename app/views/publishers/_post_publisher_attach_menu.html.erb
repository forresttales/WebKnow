	<script>

		// $(window).load( function() {
		$(document).ready(function () {				

			$(document).on('click', function(e) {
				var o_target = $(e.target).parents('.drop-story-4-attach').attr('id');
				if (o_target == null) {
					drop_story_4_attach_hide();
				}
			});

			$("#container_couple_story_4_attach").on('click', function() {
				drop_toggle_story_4_attach();
			});

			$("#div_story_4_attach_1").on('click', function() {
				btn_1_drop_add_post_publisher_image_clicked = true;
			});
			
			$("#div_story_4_attach_2").on('click', function() {
				alert('in development');
				drop_story_4_attach_hide();
			});

			$(document).on("change", "form#new_post_publisher_image input#new_post_publisher_image_image", function() {
				$("#btn_story_4_attach_text").addClass("hidden");
				$("#btn_story_4_attach_loader").removeClass("hidden");
				$("form#new_post_publisher_image input#btn_submit_post_publisher_image").click();
			});

		});
		

	</script>

