<script>

	var drop_visible_publisher_users_index102 = false;
	function drop_show_publisher_users_index102() {
		$(".drop-publisher-users-index102 .drop-102-container").removeClass('drop-hidden');
		$(".drop-publisher-users-index102 .drop-102-box").fadeIn(500).removeClass("margin-top-hide-1").addClass("margin-top-show-1");
		drop_visible_publisher_users_index102 = true;			
	}	
	function drop_hide_publisher_users_index102() {
		$(".drop-publisher-users-index102 .drop-102-box").removeClass("margin-top-show-1").addClass("margin-top-hide-1").fadeOut(50);		
		drop_visible_publisher_users_index102 = false;			
	}	
	function drop_toggle_publisher_users_index102() {
		if (drop_visible_publisher_users_index102) {
			drop_hide_publisher_users_index102();
		}
		else {
			drop_close_all_called_publisher_users_index102 = false;
			drop_show_publisher_users_index102();
		}
	}	
	
	function dropCloseAll_publisher_users_index102() {
		drop_close_all_called_publisher_users_index102 = true;
		drop_hide_publisher_users_index102();
	}
	
	
	var drop_close_all_called_publisher_users_index102 = false;
	
	
	$(document).ready(function () {				

		$("img").one("load", function() {
			if((this.id == "primary_1_image_img") || (this.id == "primary_1_image_img_generic")) {
				var img_w = this.width;
				var img_h = this.height;

				$(".container-figure-primary-1-image").css({
					// width : img_w + 4,
					// height : img_h + 4
					width : img_w,
					height : img_h						
				})// .removeClass("container-figure-image-border-load-102").addClass("container-figure-image-border-102");													
			}
			// if(this.id == "primary_1_image_generic_img") {
				// var img_w = this.width;
				// var img_h = this.height;
				// $(".container-figure-primary-1-image").css({
					// // width : img_w + 4,
					// // height : img_h + 4
					// width : img_w,
					// height : img_h						
				// }).removeClass("container-figure-image-border-load-102").addClass("container-figure-image-border-102");				
			// }				
			if(this.id == "primary_1_image_img_600") {
				var img_w = this.width;
				var img_h = this.height;
				$("#figure_dialog_102_1").css({
					width : img_w,
					height : img_h
				});																			
			}								
		}).each(function() {
		  	if(this.complete) {
		  		$(this).load();
		  		$(".drop-publisher-users-index102").removeClass("hidden");
		  	}
		});

		$(document).on('click', function(e) {
			var target_publisher_users_index102 = $(e.target).parents('.drop-publisher-users-index102').attr("class");
			if (target_publisher_users_index102 == null) {
				drop_hide_publisher_users_index102();
			}
		});

		$(".drop-publisher-users-index102 .drop-102-target").on('click', function() {
			drop_toggle_publisher_users_index102();
		});
		$(".drop-publisher-users-index102 .drop-102-link-1[data-id='1']").on("click", function() {
			dropCloseAll_publisher_users_index102();
			$(document).trigger("btn_primary_1_image_change_click");
		});
		$(".drop-publisher-users-index102 .drop-102-link-1[data-id='2']").on("click", function() {
			dropCloseAll_publisher_users_index102();
			$(document).trigger("btn_primary_1_image_crop_click");
		});
		$(".drop-publisher-users-index102 .drop-102-link-1[data-id='3']").on("click", function() {
			dropCloseAll_publisher_users_index102();
			$(document).trigger("btn_primary_1_image_delete_click");
		});
		$(".drop-publisher-users-index102 .drop-102-link-1[data-id='4']").on("click", function() {
			dropCloseAll_publisher_users_index102();
			$(document).trigger("btn_primary_1_image_new_click");
		});

		$("#primary_1_image_img").on("load", function() {
			var img_w = this.width;
			var img_h = this.height;
			$(".container-figure-primary-1-image").css({
				width : img_w,
				height : img_h						
			});													
		}).each(function() {
		  	if(this.complete) {
		  		$(this).load();
		  		$(".drop-publisher-users-index102").removeClass("hidden");
		  	}
		});

		$("#primary_1_image_img_generic").on("load", function() {
			var img_w = this.width;
			var img_h = this.height;
			$(".container-figure-primary-1-image").css({
				width : img_w,
				height : img_h						
			});													
		}).each(function() {
		  	if(this.complete) {
		  		$(this).load();
		  		$(".drop-publisher-users-index102").removeClass("hidden");
		  	}
		});

		$("#primary_1_image_img_600").on("load", function() {
			var img_w = this.width;
			var img_h = this.height;
			$("#figure_dialog_102_1").css({
				width : img_w,
				height : img_h
			});																			
		}).each(function() {
		  	if(this.complete) {
		  		$(this).load();
		  	}
		});

	});
	
</script>
