					b.push($b_held);
					b_comments.push(b);
				}
			}
		}
		var c = [];
		c.push($t);
		c.push($b);
		b_comments.push(c);
		if (!is_open) {
			$t.removeClass("add-comment-container-inactive").addClass("add-comment-container-active");
			$b.removeClass("hidden");
		}
