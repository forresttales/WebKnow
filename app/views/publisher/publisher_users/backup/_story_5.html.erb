

			<!-- <div id="scroll1" class="product-listbox" style="height: 500px;"> -->
			<div class="product-listbox">	

			<% @post_users = @log_users %>
			<% if @post_users.any? %>
				<% @nlist = 0 %>
				<% @post_users.each do |post_user| %>
