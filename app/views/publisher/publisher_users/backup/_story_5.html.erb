					<div class="table post-user-list-1" style="background-color: transparent;">
						<div class="row">
							<div class="cell-100 post-user-list-1a">
								<div class="post-user-list-1a-1">
									<ul class="publisher-product-list-grid publisher-product-list-caption">
										<li>
											<figure>
												<a href="<%= post_user.user.slug %>">
												<%= post_user_avatar_for post_user.user %>
												<figcaption>
													<!-- <span class="publisher-product-list-span"><#%= link_to "Edit", publisher_product_descriptions_path(:publisher_id => @publisher_id, :publisher_product_id => publisher_product.id), class: "" %#></span> -->
													<!-- <span><#%= link_to "Edit", { :action => :crop, :id => publisher_product.id } %#></span> -->
												</figcaption>
												</a>
											</figure>
										</li>
									<ul>
								</div>
							</div>
							<div class="cell-600">
