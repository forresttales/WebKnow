						<div class="row">
							<div class="cell-100 post-user-list-1a">
								<div class="post-user-list-1a-1">
									<ul class="publisher-product-list-grid publisher-product-list-caption">
										<li>
											<figure>
												<a href="<%= log_user.user.slug %>">
												<%= log_user_avatar_for log_user.user %>
												<figcaption>
													<!-- <span class="publisher-product-list-span"><#%= link_to "Edit", publisher_product_descriptions_path(:publisher_id => @publisher_id, :publisher_product_id => publisher_product.id), class: "" %#></span> -->
													<!-- <span><#%= link_to "Edit", { :action => :crop, :id => publisher_product.id } %#></span> -->
												</figcaption>
												</a>
											</figure>
										</li>
									<ul>
								</div>
							</div>
							<div class="cell-560 post-user-list-1b"  style="background-color: #fff;">
								<div class="post-user-list-1b-1" style="">
									<a href="<%= log_user.user.slug %>" class="post-user-span-2-a">
										<span><%= log_user.user.name_first %> </span><span><%= log_user.user.name_last %></span>
									</a>
								</div>
								<div class="post-user-list-1b-2" style="">
									<span class="post-user-span-1"><%= log_user.post_text %></span>
								</div>
								<div class="post-user-list-1b-3">
									<div class="table" style="height: 30px; margin: 0 0 10px 0;">
										<div class="row">
											<div id="render_log_user_like_<%= @nlist %>" class="cell-150">
												<%= render 'publisher_users/log_user_like' %>		
											</div>
											<div class="cell-350">
											</div>													
											<div class="cell-100" style="text-align: right;">
											    <span class="post-user-span-2">
											      - <%= time_ago_in_words(log_user.created_at) %> ago
											    </span>
											</div>													
										</div>
									</div>
								</div>
							</div>
						</div>
					</div>
					
				<% end %>

			<% else %>
				<!-- <div class="" style="color: red; padding-left: 20px;">
