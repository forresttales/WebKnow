					$t.removeClass("add-comment-container-active").addClass("add-comment-container-inactive");
					$t.html("<br>");
				}
				else {
					var b = [];
					b.push($t);
					b.push($b);
					b_comments.push(b);					
				}
			}
			a_comments = b_comments;
		}
		
