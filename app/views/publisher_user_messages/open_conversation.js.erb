<% if @messages %>
	<% if @resubscribe %>
		// Resubscribing sender (from messaging.js)
		stompResubscribe();
	<% end %>
	$('#dialog_container').html("<%= j render('publisher_user_messages/messages') %>");
	$('#dialog_id').val("<%= @dialog_id %>");
	$('#current_dialog_id').html("<%= @dialog_id %>");
<% else %>
	$('#dialog_container').html("");
	$('#current_dialog_id').html("");
	// $('#dialog_id').val("<%= @dialog_id %>");
	// $('#current_dialog_id').html("<%= @dialog_id %>");
<% end %>