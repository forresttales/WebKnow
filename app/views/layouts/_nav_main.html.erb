		$(window).scroll(function(e){ 
		  $el = $('#top-black-bar'); 
		  $topmark = $('#top-black-bar-marker');
		  $searchbar = $('#search-bar');
		
		  if ($(this).scrollTop() > $topmark.position().top+14 && $el.css('position') != 'fixed'){ 
		     $topmark.css({'height':'60px'});
		     $el.css({'position': 'fixed', 'top': '-7px','background-color': 'rgba(0,0,0,0.9);'}); 
		     $('#login').fadeOut();
		  } else if ($(this).scrollTop() < $topmark.position().top+14 && $el.css('position') == 'fixed'){
		  	$topmark.css({'height':'0px'});
		  	$el.css({'position': 'static', 'top': 'auto','background-color': 'black;'});
		  	if($el.height()<100) {
		  		$el.css({'height': '55px'});
		  	}
		  	$('#login').fadeOut();
		  }
		  if ($(this).scrollTop() > $topmark.position().top+170 && $el.height()<100) { 
		 	$('#slimlogonav').css({'display': 'block'});
		 	//$('#productnav').css({'display': 'none'});
		  } else {
		  	$('#slimlogonav').css({'display': 'none'});
		  	//$('#productnav').css({'display': 'block'});
		  	if($el.height()<100) {
		  		hideslimnav();
		  	}
		  }
		});
		
		function showslimnav() {
			$('#top-black-bar').animate({'height': '70px'},100);
			//$('#search-bar').animate({'margin-top': '7px'},100);
		  	$('#slimlogonav').addClass('navselected');
		  	$('#member_container').css({'display': 'none'});
		  	$('#slimnavcontainer').css({'display': 'block'});
		}
		
		function hideslimnav() {
			$('#top-black-bar').css({'height': '55px'});
			//$('#search-bar').animate({'margin-top': '0px'},100);
		  	$('#slimlogonav').removeClass('navselected');
		  	$('#slimnavcontainer').css({'display': 'none'});
		  	$('#member_container').css({'display': 'block'});
		}

		
	</script>




