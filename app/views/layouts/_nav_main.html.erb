	<script>
		
			function dropdown() {
				$('#dropdown').stop(true,true).slideDown(300); //Slide out the menu if it isn't opened.
			}
		
			function subnav(color,ref) {
				navAjaxContent.mouseenterSubChannel(ref); 
				$('.subnavItem:not(ref)').css('color','#000000');
				ref.style.color= color; 
			}
			
			$(document).ready(function() {
				// Populate the main navigation bar.
				for (i=0;i<topMenu.length;i++) {$('#mainNav').append('<li>' + '<a href="' + application_url + topMenu[i].url + '" onclick="navAjaxContent.intentionIntercept(\''+topMenu[i].url+'\',\''+topMenu[i].thattype+'\','+topMenu[i].id+');if(this.timer)window.clearTimeout(this.timer);return false;" onmouseover="nav(&#39;' + i + '&#39;,this); this.style.color=&#39;' + topMenu[i].hovCol + '&#39;;" onmouseout="if(this.timer)window.clearTimeout(this.timer);" class="navItem">' + topMenu[i].title + '</a></li>')};
		
				//also populate the collapsed nav
				for (i=0;i<topMenu.length;i++) {$('#slimNav').append('<li>' + '<a href="' + application_url + topMenu[i].url + '" onclick="navAjaxContent.intentionIntercept(\''+topMenu[i].url+'\',\''+topMenu[i].thattype+'\','+topMenu[i].id+');return false;">' + topMenu[i].title + '</a></li>')};
		
				// Various circumstances where the menu will fade out.	
				//$('#nav').mouseleave(function() {$('#dropdown').fadeOut(300)});
				$('#interrupt').mouseenter(function() {$('#dropdown').fadeOut(300)});
				$('#above-nav').mouseenter(function() {$('#dropdown').slideUp(300)});
		
				// Clear menu colors when you fade out the menu.
				$('#nav').mouseleave(function() {
					$('.navItem').animate({
						color:'#000000'
					})
				});
				
				$('#interrupt').mouseenter(function() {
					$('.navItem').animate({
						color:'#000000'
					})
				});
					
				$('#above-nav').mouseenter(function() {
					$('.navItem').animate({
						color:'#000000'
					})
				});
				
				$('.narrowNavigation').mouseleave(function() {
					$('.navItem').animate({
						color:'#000000'
					})
				});
		
				
				// Fade-out rotation of 'line interrupt' feature images
				//$('#interrupt-imgs').cycle();
								
			});	
			
