	<script>
		
		function openNewPwdReset() {
			// $("#form_new_pwd_reset").slideDown(400, function(){ $("#form_new_pwd_reset_a").fadeIn( 2000 ) });
			$("#form_new_pwd_reset").slideDown(400, function(){ $("#form_new_pwd_reset_a").fadeIn( 1000, function() { $("#user_new_pwd_reset_email").focus() } ) });
		}

		function closeNewPwdReset() {
			$("#form_new_pwd_reset_a").fadeOut(700, function(){ 
				$("#form_new_pwd_reset").slideUp(400); 
			});
		}


		$(window).load( function() {
		//$(document).ready(function(){

			// formValidate();
			
			// $("#user_new_pwd_reset_email").focus(function() {
				// alert('focus');
			// });
			
			// $("#user_new_pwd_reset_email").show( function() {
				// alert('show');
			// });
			
    		$("#submit_new_pwd_reset").keypress(function(e){
      			if(e.keyCode==13)
      			$("#submit_new_pwd_reset").click();
    		});							
			$("#close_form_new_pwd_reset").click(function(){
		    	$("#form_new_pwd_reset_a").fadeOut(700, function(){ $("#form_new_pwd_reset").slideUp(400) });
		  	});
		  	$("#open_form_new_pwd_reset").click(function(){
		    	$("#form_new_pwd_reset").slideDown(400, function(){ $("#form_new_pwd_reset_a").fadeIn( 2000 ) });
		  	});	
						
			$("#pwd_reset").on('click', function(){			
				
				// $("#form_new_pwd_reset").slideDown(400, function(){ $("#form_new_pwd_reset_a").fadeIn( 2000, $("#user_new_pwd_reset_email").focus() ) });
				// $("#form_new_pwd_reset").slideDown(400, function(){ $("#form_new_pwd_reset_a").fadeIn( 2000, function() { $("#user_new_pwd_reset_email").focus() } ) });
					
				openNewPwdReset();
				// $("#user_new_pwd_reset_email").trigger("focus");
				// $("#pwd_reset").blur();
				// $("#user_new_pwd_reset_email").focus();
				// $("#show_form_new_pwd_reset").slideDown(400, function(){ $("#form_new_pwd_reset").fadeIn( 2000 ) });
	    		// $("#form_new_pwd_reset, #open_edit_lesson_plan_subject").fadeOut(300, function(){
	    			// $("#show_lesson_plan_subject").addClass("hidden");
	    			// $("#open_edit_lesson_plan_subject").addClass("visibility-hidden");
	    			// $("#form_new_pwd_reset").fadeIn(1300).removeClass("hidden");
	    		// });
		  	// }).$("#user_new_pwd_reset_email").focus();
		  	});    		
						
			$("#submit_new_pwd_reset").on('click', function() {
				var input_user_new_pwd_reset_email = $("#user_new_pwd_reset_email").val();
				var obj_list = '{"email":"' + input_user_new_pwd_reset_email + 
							   '"}';      
			  	var obj = '{ "user": [' +  obj_list + '] }';                  
			  	$.ajax({
			    	type: 'POST',
			    	data : obj,
			    	// url: '/users/new_pwd_reset',
			    	url: '/password_resets/create_reset',			    	
			    	dataType: "json",
			    	contentType: "application/json",
			    	processData: false,
			    	beforeSend: function(xhr) {xhr.setRequestHeader('X-CSRF-Token', $('meta[name="csrf-token"]').attr('content'))},
	            	statusCode: {
	            	},
	            	success: function(data) {
	            		// alert(data.email);
	            		$("#pwd_reset_container_spacer").removeClass("display-none");
	            		if( data.confirm_msg_1_b) {
	            			$("#confirm_pwd_reset_2").addClass("display-none");
	            			$("#confirm_pwd_reset_3").addClass("display-none");
	            			$("#confirm_pwd_reset_1").removeClass("display-none");
	            			$("#confirm_pwd_reset_1_email").html(data.email);
	            			closeNewPwdReset();	            			
	            		}
	            		if( data.confirm_msg_2_b) {
	            			$("#confirm_pwd_reset_1").addClass("display-none");
	            			$("#confirm_pwd_reset_3").addClass("display-none");
	            			$("#confirm_pwd_reset_2").removeClass("display-none");
	            			closeNewPwdReset();
	            		}
	            		if( data.confirm_msg_3_b) {
	            			$("#confirm_pwd_reset_1").addClass("display-none");
	            			$("#confirm_pwd_reset_2").addClass("display-none");	            			
	            			$("#confirm_pwd_reset_3").removeClass("display-none");
	            			$("#confirm_pwd_reset_3_email").html(data.email);
	            		}

						// set_content_type(data.content_type, data.has_content_type_other);
						// $("#focus_30").focus();
	            	},
	            	error: function(xhr){  alert("error: " + xhr.statusText); }        
			  	});
		  		return false;
			});


            $('.tip').each(function() {
                var tipContent = $(this).next('.tip-content').html();
                $(this).qtip({
                	style: { classes: 'tooltip-text-1' },
                    content: tipContent,
                    show: 'mouseover',
                    hide: 'mouseout'
                })
            });			
			
            $('.tip-bottom-center').each(function() {
                var tipContent = $(this).next('.tip-content').html();
                $(this).qtip({
                	style: { classes: 'tooltip-text-1' },
                    content: tipContent,
                    show: 'mouseover',
                    hide: 'mouseout',
				    position: {
				        my: 'top center',
				        at: 'bottom center'
				        // target: $('.tip-bottom-center')
    				}                    
                })
            });			

            $('.tip-top-center').each(function() {
                var tipContent = $(this).next('.tip-content').html();
                $(this).qtip({
                	style: { classes: 'tooltip-text-1' },
                    content: tipContent,
                    show: 'mouseover',
                    hide: 'mouseout',
				    position: {
				        my: 'bottom center',
				        at: 'top center'
				        // target: $('.tip-top-center')
    				}                    
                })
            });			





		});

		
	</script>		
		
		
		
		
		
		
