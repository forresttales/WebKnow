<script>

	var drop_visible_namespace104 = false;
	function drop_show_namespace104() {
		$(".drop-namespace104 .drop-104-container").removeClass('drop-hidden');
		$(".drop-namespace104 .drop-104-box").fadeIn(500).removeClass("margin-top-hide-1").addClass("margin-top-show-1");
		drop_visible_namespace104 = true;			
	}	
	function drop_hide_namespace104() {
		$(".drop-namespace104 .drop-104-box").removeClass("margin-top-show-1").addClass("margin-top-hide-1").fadeOut(50);		
		drop_visible_namespace104 = false;			
	}	
	function drop_toggle_namespace104() {
		if (drop_visible_namespace104) {
			drop_hide_namespace104();
		}
		else {
			drop_close_all_called_namespace104 = false;
			drop_show_namespace104();
		}
	}	
	
	function dropCloseAll_namespace104() {
		drop_close_all_called_namespace104 = true;
		drop_hide_namespace104();
	}
	
	
	var drop_close_all_called_namespace104 = false;
	
	
	$(document).ready(function () {				

		$(document).on('click', function(e) {
			var target_namespace104 = $(e.target).parents('.drop-namespace104').attr("class");
			if (target_namespace104 == null) {
				drop_hide_namespace104();
			}
		});

		$(".drop-namespace104 .drop-104-target").on('click', function() {
			drop_toggle_namespace104();
		});
		$(".drop-namespace104 .drop-104-link-1[data-id='1']").on("click", function() {
			dropCloseAll_namespace104();
			$(document).trigger("btn_image_4_change_click");
		});
		$(".drop-namespace104 .drop-104-link-1[data-id='2']").on("click", function() {
			dropCloseAll_namespace104();
			$(document).trigger("btn_image_4_crop_click");
		});
		$(".drop-namespace104 .drop-104-link-1[data-id='3']").on("click", function() {
			dropCloseAll_namespace104();
			$(document).trigger("btn_image_4_delete_click");
		});
		$(".drop-namespace104 .drop-104-link-1[data-id='4']").on("click", function() {
			dropCloseAll_namespace104();
			$(document).trigger("btn_image_4_new_click");
		});

		$("#image_4_img").on("load", function() {
			var img_w = this.width;
			var img_h = this.height;
			$(".container-figure-image-4").css({
				width : img_w,
				height : img_h						
			});													
		}).each(function() {
		  	if(this.complete) {
		  		$(this).load();
		  		$(".drop-namespace104").removeClass("hidden");
		  	}
		});

		$("#image_4_img_generic").on("load", function() {
			var img_w = this.width;
			var img_h = this.height;
			$(".container-figure-image-4").css({
				width : img_w,
				height : img_h						
			});													
		}).each(function() {
		  	if(this.complete) {
		  		$(this).load();
		  		$(".drop-namespace104").removeClass("hidden");
		  	}
		});

		$("#image_4_img_600").on("load", function() {
			var img_w = this.width;
			var img_h = this.height;
			$("#figure_dialog_104_1").css({
				width : img_w,
				height : img_h
			});																			
		}).each(function() {
		  	if(this.complete) {
		  		$(this).load();
		  	}
		});

	});
	
</script>
