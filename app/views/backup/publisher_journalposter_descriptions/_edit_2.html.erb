		function cb_set(cntrl) {

			// curriculum_resources
			// teacher_tools             
			// school_operations        
			// content                   
			// prof_learning             

			var cb = "#publisher_journalposter_description_" + cntrl;
			var sb = "#cb_" + cntrl;;
			var column_header = false;

			if($(cb).is(':checked')) {
				
				switch (cntrl)
				{
				  case 'curriculum_resources':
					column_header = true;
				  	columnOneUnset();
				  	break;
				  case 'teacher_tools':
					column_header = true;
				  	columnTwoUnset();
				  	break;
				  case 'school_operations':
					column_header = true;
				  	columnThreeUnset();
				  	break;
				  case 'content':
					column_header = true;
				  	columnFourUnset();
				  	break;
				  case 'prof_learning':
					column_header = true;
				  	columnFiveUnset();
				  	break;
	
				  default:
				  //
				}
				
				$(cb).prop('checked', false);
				
				if (column_header) {
					$(sb).removeClass("journal-cb-background-blue");					
				}
				else {
					$(sb).removeClass("journal-cb-background-orange");
				}
			}
			else {

				switch (cntrl)
				{
				  case 'curriculum_resources':
				  	column_header = true;
				  	columnOneSet();
				  	break;
				  case 'teacher_tools':
				  	column_header = true;
				  	columnTwoSet();
				  	break;
				  case 'school_operations':
				  	column_header = true;
				  	columnThreeSet();
				  	break;
				  case 'content':
				  	column_header = true;
				  	columnFourSet();
				  	break;
				  case 'prof_learning':
				  	column_header = true;
				  	columnFiveSet();
				  	break;
				  default:
				  //
				}
				
				$(cb).prop('checked', true);
				
				if (column_header) {
					$(sb).addClass("journal-cb-background-blue");					
				}
				else {
					$(sb).addClass("journal-cb-background-orange");
				}
			}
			
			if (verify_complete()) {
				$("#arrow_1").removeClass("journal-arrow-gray").addClass("journal-arrow-green");
				$("#arrow_1").attr("disabled", false);
			}
			else {
				$("#arrow_1").removeClass("journal-arrow-green").addClass("journal-arrow-gray");
				$("#arrow_1").attr("disabled", true);
			}
		}

