			  case 'teacher_tools':
				column_header = true;
			  	break;
			  case 'school_operations':
				column_header = true;
			  	break;
			  case 'content':
				column_header = true;
			  	break;
			  case 'prof_learning':
				column_header = true;
			  	break;

			  default:
			  //
			}
			
			if(val == 0) {
				$(cb).prop('checked', false);
				// $(sb).removeClass("journal-cb-background-orange");				
			}
			else {
				$(cb).prop('checked', true);
				if (column_header) {
					$(sb).addClass("journal-cb-background-blue");
				}
				else {
					$(sb).addClass("journal-cb-background-orange");
				}
			}
		}

