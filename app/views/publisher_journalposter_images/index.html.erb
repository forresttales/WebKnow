											<span><%= link_to "Delete", destroy_image_publisher_journalposter_images_path( :publisher_id => @publisher_id, :publisher_journalposter_id => @publisher_journalposter_id, :publisher_journalposter_image_id => @publisher_journalposter_image.id, :descr => 0 ), :confirm => 'Delete this image?', :method => :post %></span>
											<!-- <span><#%= link_to '&laquo; Delete &raquo;'.html_safe, publisher_journalposter_logo, :confirm => 'Delete this photo?', :method => :delete, :params => {:publisher_journalposter_logo_id => publisher_journalposter_logo.id}, :class => 'links' %#></span> -->						   				
							   			</div>
										
										<div style="height: 30px;"></div>								        
										
