<% provide(:title, 'Publisher Profile') %>


	<style>
		
		@import url(http://www.google.com/fonts#QuickUsePlace:quickUse/Family:Istok+Web);
		
		.jp-showcell-1 {
			width: 90px;
		} 	

		td.jp-showcell {
			height: 40px;
			width: 100px;
			padding-left: 4px;
			padding-top: 4px;
			vertical-align: top;
	  		font-family: 'QuickUsePlace', sans-serif;
	  		/*font-family: 'Quattrocento Sans', sans-serif;*/
	        font-size: 10px;
	    	color: #214025;
	    	font-weight: 200;
		}
		td.jp-showcell-empty {
			height: 60px;
			width: 200px;
	    	background-color: #fff;
		}
		.jp-cell-border-top {
			border-top:#000000 solid 1px;				
		}					
		.jp-cell-border-bottom {
			border-bottom:#000000 solid 1px;				
		}					
		.jp-cell-border-left {
			border-left:#000000 solid 1px;				
		}					
		.jp-cell-border-right {
			border-right:#000000 solid 1px;				
		}					
		.jp-showcell-background-0 {
			background-color: #fff;
		}
		.jp-showcell-background-1 {
			background-color: #ddd;
		}
		span.jp-title {
			font-family: Arial, Helvetica, sans-serif;
			font-size: 20px;
			color: #D98D2D;
		}
		span.jp-title-sub {
			font-family: Arial, Helvetica, sans-serif;
			font-size: 18px;
			color: #A9D996;
		}
		.jp-btn {
		  	display: inline-block;
		  	text-decoration: none; 
		  	width: 50px;
		  	height: 20px;
		  	background: inherit;
		  	border-width: 0px;
	  		font-family: 'Quattrocento Sans', sans-serif;
	        font-size: 13px;
	    	color: #9F6720;
	    	font-weight: 400;					  	
		}
		.jp-btn:focus {
		}
		.jp-btn:hover { 
		  	text-decoration: underline;
		  	cursor: pointer; 
		}
		.jp-btn:active { 
		  	text-decoration: underline; 
		}
		.txtarea {
			font-family: Arial, Helvetica, sans-serif;
			color: #404040;			
			font-size: 14px;
			padding-left: 6px;
			padding-right: 6px;
			padding-top: 2px;
			/*border: solid 1px #478A3E;*/
			border: solid 1px #aaa;
			background-color: #fff;
			resize: none;	
			height: 70px;
			width: 400px;
		}
		.form-text-index {
			font-family: Arial, Helvetica, sans-serif;
			color: #404040;			
			font-size: 14px;
		}
		.input-form-spacer-jp {
			height: 16px;
		}
		.jp-table-descr {
			display: table; 
			width: 560px;	
		}
 		div .jp-col-right {
			display: table-cell;
			width: 410px;
			text-align: left;
			align: top;		
		}
		div .jp-col-left {
			display: table-cell;
			width: 180px;
			align: top;
			text-align: left;		
		}
		.jp-col-right span {
			font-family:Arial, Helvetica, sans-serif;
			font-size: 14px;
			color: #000;    		
			height: 16px;
		}
		.jp-col-left span {
			font-family:Arial, Helvetica, sans-serif;
			font-size: 14px;
			color: #000;    		
			height: 16px;
		}
		.jp-textarea {
			font-family:Arial, Helvetica, sans-serif;
			font-size: 14px;
			color: #000;    		
			height: 16px;
			position: relative;
			top: -48px;
		}
		
