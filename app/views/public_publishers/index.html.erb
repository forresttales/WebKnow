		var savedRange_publisher_plot,isInFocus_publisher_plot;

 		function saveSelection_publisher_plot()
		{
		    if(window.getSelection)//non IE Browsers
		    {
		        savedRange_publisher_plot = window.getSelection().getRangeAt(0);
		    }
		    else if(document.selection)//IE
		    { 
		        savedRange_publisher_plot = document.selection.createRange();  
		    } 
		}
		
		function restoreSelection_publisher_plot()
		{
		    isInFocus_publisher_plot = true;
		    document.getElementById("textarea_publisher_plot_edit").focus();
		    if (savedRange_publisher_plot != null) {
		        if (window.getSelection)//non IE and there is already a selection
		        {
		            var s = window.getSelection();
		            if (s.rangeCount > 0) 
		                s.removeAllRanges();
		            s.addRange(savedRange_publisher_plot);
		        }
		        else if (document.createRange)//non IE and no selection
		        {
		            window.getSelection().addRange(savedRange_publisher_plot);
		        }
		        else if (document.selection)//IE
		        {
		            savedRange_publisher_plot.select();
		        }
		    }
		}
		
		//this part onwards is only needed if you want to restore selection onclick
		var isInFocus_publisher_plot = false;
		function onDivBlur_publisher_plot()
		{
		    isInFocus_publisher_plot = false;
		}
		
		function cancelEvent_publisher_plot(e)
		{
		    if (isInFocus_publisher_plot == false && savedRange_publisher_plot != null) {
		        if (e && e.preventDefault) {
		            //alert("FF");
		            e.stopPropagation(); // DOM style (return false doesn't always work in FF)
		            e.preventDefault();
		        }
		        else {
		            window.event.cancelBubble = true;//IE stopPropagation
		        }
		        restoreSelection_publisher_plot();
		        return false; // false = IE style
		    }
		}
		
		function insertTextAtCursor_publisher_plot(text) {
		    var sel, range, html;
		    if (window.getSelection) {
		        sel = window.getSelection();
		        if (sel.getRangeAt && sel.rangeCount) {
		            range = sel.getRangeAt(0);
		            range.deleteContents();
		            range.insertNode( document.createTextNode(text) );
		            range.collapse(false);
		        }
		    } else if (document.selection && document.selection.createRange) {
		        document.selection.createRange().text = text;
		    }
		}			  	
			  	
		
	</script>		

